  "scripts": {
    "deps": "npx pnpm i",
    "dbPull": "npx prisma db pull --schema packages/website-server/src/prisma/schema.prisma",
    "prismaGen": "npx prisma generate --schema packages/website-server/src/prisma/schema.prisma",
    "palGen": "npx @paljs/cli schema typescript --schema packages/website-server/src/prisma/schema.prisma",
    "codegen": "npx @graphql-codegen/cli generate --target typescript --schema packages/website-server/src/prisma/schema.prisma",
    "serverBuild": "npx nx run website-server:build",
    "websiteServerDeploy": "npm run deps && npm run dbPull && npm run prismaGen && npm run palGen && npm run serverBuild",
    "buildEg": "npx nx build ethang-one",
    "eg": "node dist/packages/ethang-one/main.js",
    "prepare": "husky install"
