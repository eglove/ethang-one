{
  "name": "ethang",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "deps": "npx pnpm i",
    "buildEg": "npx nx build ethang-cli",
    "eg": "node dist/apps/ethang-cli/main.js",
    "preCommit": "npx cross-env npm run lintStaged && npm run browsersList && npm run snykDependencies && npm run snykAnalysis",
    "postCommit": "npx cross-env npm run lintAffected && npm run buildAffected && npm run testAffected && npm run e2eAffected",
    "websiteDeploy": "npx nx build website --prod --verbose",
    "lintStaged": "npx pnpm lint-staged",
    "browsersList": "npx pnpm browserslist --update-db",
    "lintAffected": "npx nx affected --target=lint --fix --base=origin/develop --head=HEAD",
    "buildAffected": "npx nx affected --target=build --base=origin/develop --head=HEAD",
    "testAffected": "npx nx affected --target=test --base=origin/develop --head=HEAD",
    "e2eAffected": "npx nx affected --target=e2e --base=origin/develop --head=HEAD",
    "snykDependencies": "npx snyk test --severity-threshold=low",
    "snykAnalysis": "npx snyk code test --severity-threshold=low",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "browserslist": [
    "extends browserslist-config-google"
  ],
