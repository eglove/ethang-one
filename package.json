    "prismaGen": "npx prisma generate --schema packages/prisma-connection/src/lib/ethang.prisma",
    "palGen": "npx @paljs/cli schema typescript --schema packages/prisma-connection/src/lib/ethang.prisma",
    "codegen": "npx @graphql-codegen/cli generate --target typescript --schema packages/prisma-connection/src/lib/ethang.prisma",
    "serverBuild": "npx nx run website-server:build",
    "websiteServerDeploy": "npm run deps && npm run dbPull && npm run prismaGen && npm run palGen && npm run serverBuild",
    "buildEg": "npx nx build ethang-one",
    "eg": "node dist/packages/ethang-one/main.js",
    "prepare": "husky install"
