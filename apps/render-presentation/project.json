{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/render-presentation",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/render-presentation",
        "outputPath": "dist/apps/render-presentation"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "render-presentation:build",
        "dev": true,
        "port": 4205
      },
      "configurations": {
        "development": {
          "buildTarget": "render-presentation:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "render-presentation:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "render-presentation:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/render-presentation"],
      "options": {
        "jestConfig": "apps/render-presentation/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/render-presentation/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
