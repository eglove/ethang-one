{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": ["!**/*", "*.d.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "excludedFiles": ["*.d.ts"],
      "rules": {
        "react/no-unknown-property": "off"
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ],
  "env": {
    "jest": true
  }
}
