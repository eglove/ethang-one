{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/use-reducer-context-example",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/use-reducer-context-example",
        "outputPath": "dist/apps/use-reducer-context-example"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "use-reducer-context-example:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "use-reducer-context-example:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "use-reducer-context-example:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "use-reducer-context-example:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/use-reducer-context-example"],
      "options": {
        "jestConfig": "apps/use-reducer-context-example/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/use-reducer-context-example/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
