{
  "root": "packages/apollo",
  "sourceRoot": "packages/apollo/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/apollo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/apollo"],
      "options": {
        "jestConfig": "packages/apollo/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
